# MQTT Configuration
# MQTT broker for receiving tool status from NEMO backend and distributing to ESP32 displays
MQTT_BROKER=10.0.0.31
MQTT_PORT=8883
MQTT_USE_SSL=true
MQTT_USERNAME=
MQTT_PASSWORD=

# ESP32 Display Configuration
# Comma-separated list of tool names that have ESP32 displays attached
ESP32_DISPLAY_TOOLS=woollam,fiji2,savannah
